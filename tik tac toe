import java.io.*;
import java.util.*;
import java.util.Scanner;
public class TicTacToePosition {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt(); 
        int index = n - 1;
        int row = index / 3;
        int col = index % 3;
        System.out.println(row + " " + col);
    }
}
